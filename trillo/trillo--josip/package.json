{
  "name": "trillo",
  "version": "1.0.0",
  "description": "trillo app",
  "main": "main.js",
  "scripts": {
    "sass": "node-sass ./sass/main.scss ./css/main.css -w --style compressed",
    "prefix:css": "postcss ./css/main.css --use autoprefixer -d ./css/prefixed/",
    "compress:css": "node-sass css/prefixed/main.css -o css/compressed --output-style compressed"
  },
  "author": "",
  "license": "ISC",
  "browserslist": "last 4 versions",
  "devDependencies": {
    "sass": "^1.77.8",
    "node-sass": "^9.0.0",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.41",
    "postcss-cli": "^11.0.0",
    "npm-run-all": "^4.1.5"
  }
}
